{
	"name": "fee-collector-scraper",
	"version": "1.0.0",
	"description": "Multi-chain Fee Collector event scraper with REST API",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc && tsc-alias",
		"start": "node dist/server.js",
		"dev": "ts-node-dev --respawn --transpile-only src/server.ts",
		"test": "NODE_ENV=test jest",
		"test:watch": "NODE_ENV=test jest --watch",
		"test:coverage": "NODE_ENV=test jest --coverage",
		"test:verbose": "NODE_ENV=test jest --verbose",
		"lint": "eslint src/**/*.ts",
		"lint:fix": "eslint src/**/*.ts --fix",
		"format": "prettier --write src/**/*.ts",
		"docker:build": "docker build -t fee-collector-scraper .",
		"docker:run": "docker run -p 3000:3000 fee-collector-scraper"
	},
	"keywords": [
		"blockchain",
		"ethereum",
		"events",
		"scraper",
		"mongodb",
		"typescript"
	],
	"author": "LI.FI Team",
	"license": "MIT",
	"dependencies": {
		"@typegoose/typegoose": "^12.0.1",
		"@types/swagger-jsdoc": "^6.0.4",
		"@types/swagger-ui-express": "^4.1.8",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.3.1",
		"ethers": "^5.7.2",
		"express": "^4.18.2",
		"express-rate-limit": "^7.5.1",
		"helmet": "^7.2.0",
		"ioredis": "^5.6.1",
		"joi": "^17.13.3",
		"mongoose": "^8.0.3",
		"morgan": "^1.10.0",
		"swagger-jsdoc": "^6.2.8",
		"swagger-ui-express": "^5.0.1",
		"tsconfig-paths": "^4.2.0",
		"winston": "^3.11.0"
	},
	"devDependencies": {
		"@lifi/types": "^17.19.1",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.21",
		"@types/ioredis": "^4.28.10",
		"@types/jest": "^29.5.8",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.10.5",
		"@types/supertest": "^2.0.16",
		"@typescript-eslint/eslint-plugin": "^6.15.0",
		"@typescript-eslint/parser": "^6.15.0",
		"eslint": "^8.56.0",
		"jest": "^29.7.0",
		"prettier": "^3.1.1",
		"supertest": "^6.3.3",
		"ts-jest": "^29.1.1",
		"ts-node-dev": "^2.0.0",
		"tsc-alias": "^1.8.16",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
